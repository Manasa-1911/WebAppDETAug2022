@{
}

@*<div class="table-responsive">*@
    <p>Friend Details</p>
    <table border="1">
    <tr>
        <th>Friend ID</th>
        <th>Friend Name</th>
        <th>Place</th>
    </tr>
        @foreach(Friend p in @Model)
        {
            <tr>
            <td>@p.id</td>
            <td>@p.FriendName</td>
            <td>@p.Place</td>
        </tr>
        }
    </table>
</div>